{
    "problemMatcher": [
        {
            "owner": "rustfmt",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(Diff in (.+)) at line (\\d+):$",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        }
    ]
}
